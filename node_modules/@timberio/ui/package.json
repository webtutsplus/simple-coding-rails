{
  "name": "@timberio/ui",
  "version": "2.7.0",
  "description": "A user-interface library built for React",
  "author": "Timber",
  "license": "MIT",
  "homepage": "https://github.com/timberio/ui#readme",
  "repository": {
    "type": "git",
