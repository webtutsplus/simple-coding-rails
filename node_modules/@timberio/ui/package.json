    "react-dom": "^16.7.0",
    "storybook-addon-jsx": "^6.0.0",
    "typescript": "^3.2.2"
  },
  "eslintConfig": {
    "extends": "timber"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "setupFiles": [
      "./.jest/jest.setup.js"
    ]
  }
