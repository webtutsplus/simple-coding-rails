    "blacklist": "^1.1.4",
    "classnames": "^2.2.6",
    "clean-element": "^1.0.2",
    "deepmerge": "^3.0.0",
    "emotion": "^9.2.12",
    "emotion-theming": "^9.2.9",
    "md5": "^2.2.1",
    "object-path": "^0.11.4",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "react-content-loader": "^3.1.2",
    "react-emotion": "^9.2.12",
    "react-feather": "^1.1.5",
